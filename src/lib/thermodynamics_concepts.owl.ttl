            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:value ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty thmo_concepts:value ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty thmo_concepts:value ],
        thmo_concepts:UniversalQuantity,
        thmo_concepts:Variable ;
    skos:altLabel "standard pressure" ;
    skos:inScheme <https://example.org/thmo/concepts> .

thmo_concepts:T_const a owl:DatatypeProperty ;
    rdfs:label "T_const" ;
