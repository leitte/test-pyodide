thmo_equations:IsentropicPerfectGasTemperatureEquation a owl:Class ;
    rdfs:label "IsentropicPerfectGasTemperatureEquation" ;
    ns1:mathExpression "T_0 / T_1 = (p_0/p_1) ** ((kappa - 1)/kappa)" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:as_text ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty thmo_concepts:as_text ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty thmo_concepts:as_text ],
        thmo_equations:IsentropicPerfectGasEquation ;
    skos:inScheme <https://example.org/thmo/equations> .

thmo_equations:IsobaricPerfectGasEquation a owl:Class ;
    rdfs:label "IsobaricPerfectGasEquation" ;
    rdfs:subClassOf thmo_equations:IsobaricRule,
