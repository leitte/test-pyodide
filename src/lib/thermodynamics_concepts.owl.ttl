<https://example.org/thmo/concepts.owl.ttl> a owl:Ontology ;
    rdfs:label "thermo_concepts" ;
    dcterms:license "https://creativecommons.org/publicdomain/zero/1.0/" ;
    dcterms:title "Thermodynamics Ontology" ;
    pav:version "2.0.0" ;
    skos:definition "Ontology to describe classes relating to thermodynamics." .

thmo_concepts:ClosedSystem a owl:Class ;
    rdfs:label "ClosedSystem" ;
    rdfs:subClassOf thmo_concepts:System ;
    skos:altLabel "closed system",
        "control mass" ;
    skos:definition "A closed thermodynamic system contains a macrocopic amount of matter.  There is no mass flow across its boundary, the mass of the system is constant.  Work and heat can be transferred across the system's boundary. The boundary  can be a real object (e.g. walls) or we can just imagine it. The system can  move freely in space. Everything outside the system is called \"surroundings\"." ;
    skos:inScheme <https://example.org/thmo/concepts> .

thmo_concepts:OpenSystem a owl:Class ;
    rdfs:label "OpenSystem" ;
    rdfs:subClassOf thmo_concepts:System ;
    skos:altLabel "control volume",
        "open system" ;
    skos:definition "An open system is a fixed part of the space that we are interested in.  Work and heat as well as matter can be transferred across its boundary. " ;
    skos:inScheme <https://example.org/thmo/concepts> .

thmo_concepts:Problem a owl:Class ;
    rdfs:label "Problem" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:required ],
        [ a owl:Restriction ;
            owl:allValuesFrom thmo_concepts:Transition ;
            owl:onProperty thmo_concepts:transitions ],
        [ a owl:Restriction ;
            owl:allValuesFrom thmo_concepts:System ;
            owl:onProperty thmo_concepts:systems ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty thmo_concepts:required ],
        [ a owl:Restriction ;
            owl:allValuesFrom thmo_concepts:PureMaterial ;
            owl:onProperty thmo_concepts:pureMaterials ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:transitions ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:change_of_states ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:states ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:additional_equations ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:mixedMaterials ],
        [ a owl:Restriction ;
            owl:allValuesFrom thmo_concepts:Mixture ;
            owl:onProperty thmo_concepts:mixedMaterials ],
        [ a owl:Restriction ;
            owl:allValuesFrom thmo_concepts:State ;
            owl:onProperty thmo_concepts:states ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty thmo_concepts:additional_equations ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:systems ],
        [ a owl:Restriction ;
            owl:allValuesFrom thmo_concepts:ChangeOfState ;
            owl:onProperty thmo_concepts:change_of_states ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:pureMaterials ] ;
    skos:inScheme <https://example.org/thmo/concepts> .

thmo_concepts:StandardPressure a owl:Class ;
    rdfs:label "StandardPressure" ;
