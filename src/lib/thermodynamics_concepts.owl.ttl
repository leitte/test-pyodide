            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:as_text ],
        [ a owl:Restriction ;
            owl:allValuesFrom thmo_concepts:ChangeOfState ;
            owl:onProperty thmo_concepts:change_of_state ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty thmo_concepts:change_of_state ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty thmo_concepts:as_text ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty thmo_concepts:as_text ],
        thmo_equations:AdiabaticRule,
        thmo_equations:Inequality,
        thmo_equations:IrreversibleRule ;
    skos:inScheme <https://example.org/thmo/equations> .

thmo_equations:isentropic_perfect_gas_temperature_equation a owl:ObjectProperty ;
    rdfs:label "isentropic_perfect_gas_temperature_equation" ;
    rdfs:range thmo_equations:IsentropicPerfectGasTemperatureEquation ;
    skos:inScheme <https://example.org/thmo/equations> .

